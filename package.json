    "generate": "yarn generate:types && yarn generate:gql",
    "generate:types": "bash ./scripts/cf-generator.sh",
    "generate:gql": "graphql-codegen --config helpers/codegen.ts",
    "dev": "next dev",
    "build": "next build",
    "build:storybook": "storybook build",
    "start": "next start",
    "start:storybook": "storybook dev -p 6006",
